{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "demo",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "data": {
      "projectType": "library",
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "prefix": "libtest",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/data/tsconfig.lib.json",
            "project": "libs/data/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/data/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/data/**/*.ts"
            ]
          }
        }
      }
    },
    "ngx-doc-viewer": {
      "projectType": "library",
      "root": "libs/ngx-doc-viewer",
      "sourceRoot": "libs/ngx-doc-viewer/src",
      "prefix": "libtest",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ngx-doc-viewer/tsconfig.lib.json",
            "project": "libs/ngx-doc-viewer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ngx-doc-viewer/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ngx-doc-viewer/src/**/*.ts",
              "libs/ngx-doc-viewer/src/**/*.html"
            ]
          }
        }
      }
    }
  }
}